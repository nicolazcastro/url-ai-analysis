{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Seeder Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/sync-sequelize.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Command Line Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/index.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch  Web Server",
            "skipFiles": [
                "<node_internals>/**",
                "url-output/**/*"
            ],
            "runtimeExecutable": "node",
            "runtimeArgs": [
                "--inspect=9229",
                "${workspaceFolder}/index.js"
            ],
            "env": {
                "NODE_ENV": "development"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch User Microservice",
            "skipFiles": [
                "<node_internals>/**",
                "url-output/**/*"
            ],
            "runtimeExecutable": "node",
            "runtimeArgs": [
                "--inspect=9229",
                "${workspaceFolder}/deploy/frontend/index.js"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "/Users/nicolascastro/Development/url-ai-analysis/deploy/microservice-user"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch FrontEnd Microservice",
            "skipFiles": [
                "<node_internals>/**",
                "url-output/**/*"
            ],
            "runtimeExecutable": "node",
            "runtimeArgs": [
                "--inspect=9229",
                "${workspaceFolder}/index.js"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "/Users/nicolascastro/Development/url-ai-analysis/deploy/frontend"
        }
    ]
}